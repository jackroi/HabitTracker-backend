{
  "name": "habit-tracker-backend",
  "version": "0.0.1",
  "description": "Backend that provides REST API for HabitTracker system",
  "main": "src/server.ts",
  "scripts": {
    "start": "node ./dist/src/server.js",
    "compile": "tsc",
    "dev": "tsc-watch --noClear --onSuccess \"node ./dist/src/server.js\"",
    "init-db": "node dist/src/init-db.js",
    "test": "jest",
    "postversion": "git push && git push --tags",
    "heroku-postbuild": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackroi/HabitTracker-backend.git"
  },
  "keywords": [
    "backend",
    "rest-api",
    "mongodb",
    "express",
    "typescript"
  ],
  "author": "Giacomo Rosin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jackroi/HabitTracker-backend/issues"
  },
  "homepage": "https://github.com/jackroi/HabitTracker-backend#readme",
  "engines": {
    "node": "14.16.1"
  },
  "dependencies": {
    "console-error": "0.0.4",
    "console-info": "0.0.5",
    "console-warn": "0.0.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^2.0.1",
    "mongoose": "^5.12.5",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "socket.io": "^4.1.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-jwt": "^6.0.1",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/luxon": "^1.27.1",
    "@types/node": "^14.14.41",
    "@types/passport": "^1.0.6",
    "@types/passport-http": "^0.3.8",
    "@types/supertest": "^2.0.11",
    "jest": "^27.2.3",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.4.3"
  },
  "private": true
